<!-- Simple linear flowchart: Start → Process → End -->
<?xml version="1.0" encoding="UTF-8"?>
<mxfile host="Electron">
  <diagram name="Flowchart" id="flowchart-tpl">
    <mxGraphModel dx="1200" dy="800" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="0" pageScale="1" pageWidth="850" pageHeight="1100" math="0" shadow="0" defaultFontFamily="Arial">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- EDGES FIRST (renders behind shapes) -->
        <mxCell id="e1" edge="1" source="start" target="process" parent="1" style="edgeStyle=orthogonalEdgeStyle;fontFamily=Arial;fontSize=16;strokeColor=#4a5568;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e2" edge="1" source="process" target="end" parent="1" style="edgeStyle=orthogonalEdgeStyle;fontFamily=Arial;fontSize=16;strokeColor=#4a5568;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- VERTICES -->
        <mxCell id="start" vertex="1" parent="1" value="Start" style="ellipse;fillColor=#2d3748;strokeColor=#4a5568;fontColor=#e2e8f0;fontFamily=Arial;fontSize=16;">
          <mxGeometry x="140" y="60" width="160" height="44" as="geometry"/>
        </mxCell>

        <mxCell id="process" vertex="1" parent="1" value="Process Step" style="rounded=1;fillColor=#1a365d;strokeColor=#2b6cb0;fontColor=#bee3f8;fontFamily=Arial;fontSize=16;">
          <mxGeometry x="140" y="164" width="160" height="44" as="geometry"/>
        </mxCell>

        <mxCell id="end" vertex="1" parent="1" value="End" style="ellipse;fillColor=#2d3748;strokeColor=#4a5568;fontColor=#e2e8f0;fontFamily=Arial;fontSize=16;">
          <mxGeometry x="140" y="268" width="160" height="44" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
